                                                                            <li>

                                                                                <a asp-controller="Home"
                                                                                   asp-action="FAQ">
                                                                                    FAQs
                                                                                </a>

                                                                            </li>
                                                                            <li><a asp-action="Index" asp-controller="Concat">@ResHeader.Contact</a></li>

                                                                        </ul>
                                                                    </div>
                                                                </div>
                                                            </div>
                                                        </div>
                                                    </div>
                                                </li>
                                            </ul>
                                        </li>
                                        <li>
                                            <a asp-action="List" asp-controller="Reviews">@ResHeader.Reviews</a>
                                        </li>
                                        <li>
                                            <a asp-controller="About" asp-action="Index">@ResHeader.About_us</a>
                                        </li>
                                    </ul>
                                </nav>
                            </div>
