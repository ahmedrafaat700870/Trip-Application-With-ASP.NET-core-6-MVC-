@model InformationModel
<!-- footer section start -->
<footer class="sticky-footer  blur-up lazyload">
    <section class="section-b-space darken-layout">
        <div class="container">
            <div class="row footer-theme partition-f">
                <div class="col-lg-4 col-md-6">
                    <div class="footer-title footer-mobile-title">
                        <h4>über uns</h4>
                    </div>
                    <div class="footer-contant">
                        <div class="footer-logo">
                            <img src="~/Uploads/HomePage/Logo/@Model.LogoImage" alt="Logo">
                        </div>
                        <p>
                            @Model.FooterDescription
                        </p>
                        <div class="footer-social">
                            <ul>
                                <li>
                                    <a href="@Model.FaceBookAccount"><i class="fa fa-facebook"
                                            aria-hidden="true"></i></a>
                                </li>
                                <li>
                                    <a href="https://@Model.InformationEmail"><i class="fa fa-google-plus"
                                            aria-hidden="true"></i></a>
                                </li>
                                <li>
                                    <a href="@Model.TwitterAcount"><i class="fa fa-twitter" aria-hidden="true"></i></a>
                                </li>
                                <li>
                                    <a href="@Model.InstagramAccount"><i class="fa fa-instagram"
                                            aria-hidden="true"></i></a>
                                </li>
                                @*
                                    <li>
                                    <a ><i class="fa fa-rss" aria-hidden="true"></i></a>
                                    </li>
                                *@
                            </ul>
                        </div>
                    </div>
                </div>
                <div class="col offset-xl-1 ">
                    <div class="sub-title">
                        <div class="footer-title">
                            <h4>Unsre Social Media</h4>
                        </div>
                        <div class="footer-contant">
                            <ul>
                                <li><a href="https://@Model.InformationEmail">google</a></li>
                                <li><a href="@Model.TwitterAcount">twitter</a></li>
                                <li><a href="@Model.InstagramAccount">instagram</a></li>
                                <li><a href="@Model.FaceBookAccount">facebook</a></li>
                                @*
                                    <li><a >featured</a></li>
                                *@
                            </ul>
                        </div>
                    </div>
                </div>
                <div class="col">
                    <div class="sub-title">
                        <div class="footer-title">
                            <h4>Warum bei uns buchen ?</h4>
                        </div>
                        <div class="footer-contant">
                            <ul>
                                <li><a >Sun & Sea</a></li>
                                <li><a >Museums</a></li>
                                <li><a >Culture and Historical Sites</a></li>
                                <li><a >Nile Cruise</a></li>
                                <li><a >Ecotourism and nature</a></li>
                            </ul>
                        </div>
                    </div>
                </div>
                <div class="col">
                    <div class="sub-title">
                        <div class="footer-title">
                            <h4>@Model.WebSiteName information</h4>
                        </div>
                        <div class="footer-contant">
                            <ul class="contact-list">
                                <li>
                                    <i class="fa fa-map-marker"></i>Lage, @Model.Location
                                </li>
                                <li><i class="fa fa-phone"></i>Rufen Sie uns an : @Model.InformationPhone</li>
                                <li><i class="fa fa-envelope-o"></i>Email : <a
                                        href="@Model.InformationEmail">@Model.InformationEmail</a></li>
                                <li><i class="fa fa-fax"></i>Fax: @Model.Facx</li>
                            </ul>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </section>
    <div class="sub-footer dark-subfooter">
        <div class="container">
            <div class="row">
                <div class="col-xl-6 col-md-6 col-sm-12">
                    <div class="footer-end">
                        <p>
                            <i class="fa fa-copyright" aria-hidden="true"></i> 2022 powered by
                            ahmed rafaat
                        </p>
                    </div>
                </div>
                <div class="col-xl-6 col-md-6 col-sm-12">
                    <div class="payment-card-bottom">
                        <ul>
                            <li>
                                <a ><img src="~/MainPage/assets/images/icon/visa.png" alt=""></a>
                            </li>
                            <li>
                                <a ><img src="~/MainPage/assets/images/icon/mastercard.png" alt=""></a>
                            </li>
                            <li>
                                <a ><img src="~/MainPage/assets/images/icon/paypal.png" alt=""></a>
                            </li>
                            <li>
                                <a ><img src="~/MainPage/assets/images/icon/american-express.png" alt=""></a>
                            </li>
                            <li>
                                <a ><img src="~/MainPage/assets/images/icon/discover.png" alt=""></a>
                            </li>
                        </ul>
                    </div>
                </div>
            </div>
        </div>
    </div>
</footer>
<!-- footer section end -->
